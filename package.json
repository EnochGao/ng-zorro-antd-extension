{
  "name": "ng-zorro-antd-extension",
  "version": "21.0.0",
  "license": "MIT",
  "description": "An extension based on ng-zorro-antd",
  "keywords": [
    "angular",
    "ng-zorro-antd",
    "frontend",
    "extension",
    "ng-zorro-antd-extension"
  ],
  "homepage": "https://ng-zorro-antd-extension.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnochGao/ng-zorro-antd-extension.git"
  },
  "author": {
    "name": "enochgao",
    "email": "enochgao@qq.com"
  },
  "scripts": {
    "ng": "npx ng",
    "start": "npx docgeni serve --port 4600",
    "build": "npx docgeni build --production",
    "build:lib": "npx ng build ng-zorro-antd-extension",
    "build:components": "cd components && npm run build && npm run postbuild",
    "build:all": "npm-run-all \"build:lib\" \"build:components\" \"deploy\"",
    "deploy": "cd dist/ng-zorro-antd-extension && npm publish",
    "watch": "npx ng build --watch --configuration development",
    "ui": "npx ng serve ui",
    "test": "npx ng test",
    "lint": "npx ng lint --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "^21.0.8",
    "@angular/common": "^21.0.8",
    "@angular/compiler": "^21.0.8",
    "@angular/core": "^21.0.8",
    "@angular/forms": "^21.0.8",
    "@angular/platform-browser": "^21.0.8",
    "@angular/platform-browser-dynamic": "^21.0.8",
    "@angular/router": "^21.0.8",
    "@commitlint/format": "^19.3.0",
    "@ctrl/tinycolor": "^4.2.0",
    "@vivliostyle/core": "^2.29.0",
    "@wangeditor-next/editor": "^5.6.49",
    "@wangeditor/editor": "^5.1.23",
    "conventional-changelog-atom": "^5.0.0",
    "date-fns": "^2.29.2",
    "ng-zorro-antd": "^21.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "^21.0.5",
    "@angular-devkit/schematics": "^21.0.5",
    "@angular-eslint/builder": "21.1.0",
    "@angular-eslint/eslint-plugin": "21.1.0",
    "@angular-eslint/eslint-plugin-template": "21.1.0",
    "@angular-eslint/schematics": "21.1.0",
    "@angular-eslint/template-parser": "21.1.0",
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.8",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@docgeni/cli": "2.7.0-next.0",
    "@docgeni/template": "2.7.0-next.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@typescript-eslint/utils": "^7.2.0",
    "angular-eslint": "^21.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "8.57.0",
    "husky": "^9.0.11",
    "jasmine-core": "~4.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "less": "^4.2.0",
    "ng-packagr": "^21.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.9.2"
  }
}
